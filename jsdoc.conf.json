{
  "docdash": {
    "collapse": true,
    "menu": {
      "Podfeet Podcasts": {
        "href": "https://podfeet.com/",
        "target": "_blank"
      },
        "Programming by Stealth": {
            "href":"https://pbs.bartificer.net",
            "target": "_blank"
        },
        "JSDoc": {
            "href":"https://jsdoc.app/index.html",
            "target": "_blank"
        }
    },
    "search": true,
    "sectionOrder": ["Modules", "Classes", "Tutorials"],
    "static": true,
    "typedefs": true
  },
  "opts": {
    "encoding": "utf8",
    "readme": "README.md",
    "template": "./node_modules/docdash"
  },
  "source": {
    "include": [
      "./src/",
      "src/script.mjs",
      "README.md"
    ]
  }, 
  "plugins": [
      "plugins/markdown"
  ],
  "tags": {
      "allowUnknownTags": false,
      "dictionaries": ["jsdoc","closure"]
  }
}